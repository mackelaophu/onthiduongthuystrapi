{
  "kind": "collectionType",
  "collectionName": "invite_codes",
  "info": {
    "singularName": "invite-code",
    "pluralName": "invite-codes",
    "displayName": "Mã mời học",
    "description": "Mã mời thuộc bộ Access Code"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "minLength": 5,
      "maxLength": 5,
      "required": true,
      "unique": true
    },
    "redeemed": {
      "type": "boolean",
      "default": false
    },
    "access_code": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::access-code.access-code",
      "inversedBy": "invite_codes"
    },
    "jwt": {
      "type": "string"
    }
  }
}
