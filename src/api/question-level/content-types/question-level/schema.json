{
  "kind": "collectionType",
  "collectionName": "question_levels",
  "info": {
    "singularName": "question-level",
    "pluralName": "question-levels",
    "displayName": "Trình độ",
    "description": "Loại hình / Trình độ: Thủy thủ, Thợ máy, Lái phương tiện"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question",
      "mappedBy": "level"
    },
    "exam_sets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam-set.exam-set",
      "mappedBy": "level"
    }
  }
}
